{
  "extends": "@finsweet/tsconfig",
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": "./",
    "paths": {
      "$app": ["src/app"],
      "$config/*": ["src/config/*"],
      "$events": ["src/types"],
      "$types": ["src/types"],
      "$utils/*": ["src/utils/*"]
    },
    "types": ["@finsweet/ts-utils", "gsap"]
  }
}
